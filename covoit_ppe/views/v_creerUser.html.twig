{%extends "layout.html.twig"%}
{% block content %}
    <article>
    {{ form_start(form)}}
    {{form_errors(form)}}
    {{form_row(form.nom)}}</br>
    {{form_row(form.prenom)}}</br>
    {{form_row(form.email)}}</br>
    {{form_row(form.telephone)}}</br>
    {{form_row(form.motDePasse)}}</br>
    {{form_row(form.creerUnUtilisateur)}}
    {{form_end(form)}}
    </article>
{% endblock %}